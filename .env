# Environment Variables para desenvolvimento
# IMPORTANTE: Não commitar este arquivo!

# Database (Supabase) - valores de exemplo para desenvolvimento
SUPABASE_URL=https://example.supabase.co
SUPABASE_KEY=example_anon_key
SUPABASE_SERVICE_ROLE_KEY=example_service_role_key

# LLM APIs - valores de exemplo
GOOGLE_API_KEY=example_google_api_key
OPENAI_API_KEY=example_openai_api_key

# JWT Secret - valor de exemplo (gere um novo para produção)
JWT_SECRET_KEY=example_jwt_secret_key_for_development_only
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=15
REFRESH_TOKEN_EXPIRE_DAYS=7

# Redis Cache
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Email Configuration
SENDGRID_API_KEY=example_sendgrid_key
FROM_EMAIL=noreply@example.com
FROM_NAME=SkyNET-I2A2

# Application Settings
APP_NAME=SkyNET-I2A2
APP_VERSION=1.0.0
DEBUG=True
LOG_LEVEL=INFO

# Security
BCRYPT_ROUNDS=12
LOGIN_ATTEMPTS_LIMIT=5
LOGIN_ATTEMPTS_WINDOW=600

# File Upload
MAX_FILE_SIZE_MB=10
ALLOWED_EXTENSIONS=["pdf","xml","csv","xlsx","xls"]

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# External Services
WEBHOOK_URL=
AUDIT_ENABLED=True